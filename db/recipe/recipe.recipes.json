      }
    },
    {
      "name": "julienned carrots",
      "quantity": "1 cup",
      "id": {
        "$oid": "647b446c57ed6b5ff901f267"
      }
    },
    {
      "name": "sliced snap peas",
      "quantity": "1 cup",
