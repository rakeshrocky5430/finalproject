      }
    },
    {
      "name": "reduced-sodium soy sauce",
      "quantity": "2 tablespoons",
      "id": {
        "$oid": "647b444b57ed6b5ff901f264"
      }
    },
    {
      "name": "ketchup",
      "quantity": "2 tablespoons",
      "id": {
        "$oid": "647b445657ed6b5ff901f265"
      }
    },
    {
      "name": "cooked boneless, skinless chicken breast, shredded",
      "quantity": "8 ounces",
      "id": {
